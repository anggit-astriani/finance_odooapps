async makeRpcWithGeolocation(route,params){if(!isIosApp()){return new Promise((resolve)=>{navigator.geolocation.getCurrentPosition(async({coords:{latitude,longitude}})=>{const result=await this.rpc(route,{...params,latitude,longitude,});resolve(result);},async(err)=>{const result=await this.rpc(route,{...params});resolve(result);},{enableHighAccuracy:true});});}
else{return this.rpc(route,{...params})}}
async onManualSelection(employeeId,enteredPin){const result=await this.makeRpcWithGeolocation('manual_selection',{'token':this.props.token,'employee_id':employeeId,'pin_code':enteredPin})
